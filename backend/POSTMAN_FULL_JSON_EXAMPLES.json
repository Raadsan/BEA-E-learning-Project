{
  "endpoint_1_single_subprogram": {
    "method": "POST",
    "url": "http://localhost:5000/api/classes/create-for-subprogram",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "subprogram_id": 1,
      "teacher_id": null,
      "description": "Morning and Night classes for this subprogram"
    }
  },
  "endpoint_2_all_subprograms": {
    "method": "POST",
    "url": "http://localhost:5000/api/classes/create-for-all-subprograms",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "teacher_id": null,
      "description": "Morning and Night classes for all subprograms"
    }
  },
  "example_with_teacher": {
    "method": "POST",
    "url": "http://localhost:5000/api/classes/create-for-subprogram",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "subprogram_id": 1,
      "teacher_id": 5,
      "description": "Morning and Night classes assigned to teacher"
    }
  },
  "example_all_subprograms_with_teacher": {
    "method": "POST",
    "url": "http://localhost:5000/api/classes/create-for-all-subprograms",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "teacher_id": 5,
      "description": "Morning and Night classes for all subprograms assigned to teacher"
    }
  }
